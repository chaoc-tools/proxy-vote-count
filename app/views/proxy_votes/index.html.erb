<h1>Listing proxy_votes</h1>
Total Count: <%= @total_vote_count %>
Vote by building: <%= @vote_count_by_building %>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Count</th>
      <th>Building</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proxy_votes.each do |proxy_vote| %>
      <tr>
        <td><%= proxy_vote.year %></td>
        <td><%= proxy_vote.count %></td>
        <td><%= Building.find(proxy_vote.building_id).name %></td>
        <td><%#= link_to 'Show', proxy_vote %></td>
        <td><%#= link_to 'Edit', edit_proxy_vote_path(proxy_vote) %></td>
        <td><%#= link_to 'Destroy', proxy_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'New Proxy vote', new_proxy_vote_path %>
